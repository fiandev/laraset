{
  "name": "@fiandev/laraset",
  "version": "1.0.0",
  "description": "Automated Laravel project setup with Docker and SQLite",
  "main": "dist/index.js",
  "bin": {
    "laraset": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node dist/index.js --help",
    "lint": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "laravel",
    "docker",
    "setup",
    "automation",
    "php",
    "sqlite"
  ],
  "author": "fiandev",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.10.0",
    "typescript": "^5.0.0",
    "semantic-release": "^24.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^10.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "commander": "^14.0.2",
    "semver": "^7.7.3",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fiandev/laraset.git"
